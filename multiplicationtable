class Main {
  public static void main(String[] args) {
    System.out.println("Here is a multiplication table from numbers 1-12");
    System.out.println();
    for (int i = 1; i <= 12; i++) {
      for (int y = 1; y <= 12; y++) {
        System.out.print(y * i + " ");
      }
      System.out.println();
    }
  }
}
